<ViewpointsView>
  <Group Orientation="Vertical" Spacing="5px" Alignment="Top" ContentAlignment="Center">
    <!-- <Label Text="{MenuHeader}" /> -->

    <List Orientation="Vertical" Alignment="Top" Items="{Viewpoints}"
          Height="540px" VerticalSpacing="5px"
          IsScrollable="true"
          CanSelect="true" ItemSelected="ViewpointChanged"
          Highlighted-ItemColor="Orange">
      <ListItem IsTemplate="true" BackgroundColor="Gray80">
        <Label Text="{#Item.Name}" TextAlignment="Center" />
      </ListItem>
    </List>

  </Group>
</ViewpointsView>
